        <Panel name="table_create" img="table_panel" pos="500, 500" scale="0.50, 0.50" buttons="play" inputs="ip" bars="healh"/>
    </Panels>
    <Scenes>
        <Scene name="menu" panels="background, home_panel"/>
        <Scene name="create" panels="background, create_game_panel"/>
        <Scene name="setting" panels="background, settings_panel"/>
        <Scene name="credit" panels="background, credits_panel"/>
